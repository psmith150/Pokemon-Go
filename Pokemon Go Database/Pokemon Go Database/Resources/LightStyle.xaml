    <!--#region IVSelectionSlider-->
    <Color x:Key="SliderThumbGradientStart">LightGray</Color>
    <Color x:Key="SliderThumbGradientEnd">LightGray</Color>
    <SolidColorBrush x:Key="SliderTickBarFill" Color="Black"/>
    <SolidColorBrush x:Key="SliderThumbBorder" Color="Black" Opacity="0.2"/>
    <SolidColorBrush x:Key="SliderTrackFill" Color="CornflowerBlue" Opacity="0.75"/>
